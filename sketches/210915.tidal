setcps (128/240)

d1 $
  (+ note "[0 2]/16") $
  every 6 (# crush 4) $
  whenmod 10 7 (jux (iter 4)) $
  every 7 (# coarse "4 8 2") $
  note "[~ 0 <0!3 3> <0!3 1>]*4"
  # s "bbass:44"
