    # s "bhh:47"
    ,
    n "[~ 0]*2"
    # s "bsnare:64"
    ,
    mask "[f t]/4" $
    n "<[~ [~ [~ 0]] ~ ~] [~ [~ ~ ~ 0] [~ 0 ~ ~] [0 ~ 0 0]]>"
    # s "bsnare:63"
  ]
  # shape 0.5

d3 $
  stack [
    n "[~ 0]*2"
    # s "bperc:20"
    ,
    mask "[f t]/2" $
    n "~ ~ ~ [~ ~ ~ 0]"
    # s "bperc:19"
    ,
    mask "[f f f t]/4" $
    fast 2 $
    mask "[f t]/2" $
    n "~ 0 ~ 0 ~ 0 0 0"
    # s "bperc:57"
  ]
  # gain 0.9
